{
  "name": "@groundworx/components",
  "version": "1.0.2",
  "description": "Reusable React components for Groundworx blocks and themes.",
  "author": "Johanne Courtright",
  "license": "MIT",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "babel src --out-dir dist --extensions \".js,.jsx\"",
    "build:watch": "npm run build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/groundworx-dev/components.git"
  },
  "keywords": [
    "wordpress",
    "groundworx",
    "components",
    "blocks",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/groundworx-dev/components/issues"
  },
  "homepage": "https://github.com/groundworx-dev/components#readme",
  "peerDependencies": {
    "@wordpress/block-editor": "*",
    "@wordpress/components": "*",
    "@wordpress/element": "*",
    "@wordpress/i18n": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.1",
    "@wordpress/babel-preset-default": "^8.23.0"
  },
  "dependencies": {
    "@groundworx/utils": "^1.0.2"
  }
}
